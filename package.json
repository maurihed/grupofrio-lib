{
  "name": "grupofrio",
  "version": "1.0.0",
  "description": "Grupo frio",
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "prod": "yarn webpack -- --env.mode production",
    "dev": "yarn webpack-dev-server -- --env.mode development --hot",
    "start": "yarn webpack-dev-server -- --env.mode development --hot",
    "start:rutas": "yarn webpack-dev-server -- --env.mode development --env.base rutas-frecuencias --hot",
    "start:localizador": "yarn webpack-dev-server -- --env.mode development --env.base localizador --hot",
    "prod:typescript": "yarn prod -- --env.presets typescript",
    "prod:analyze": "yarn prod -- --env.presets analyze",
    "prod:compress": "yarn prod -- --env.presets compress",
    "prod:debug": "yarn debug -- --env.mode production",
    "dev:debug": "yarn debug -- --env.mode development"
  },
  "author": "Maurihed",
  "license": "MIT",
  "dependencies": {
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "materialize-css": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "assets-webpack-plugin": "^5.0.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.3.0",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.26.8",
    "sass-loader": "^13.2.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^9.4.2",
    "typescript": "^3.9.5",
    "vue": "^2.6.11",
    "vue-loader": "^17.0.1",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.6.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
